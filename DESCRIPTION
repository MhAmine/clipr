Package: clipr
Type: Package
Title: Read and Write from the System Clipboard
Version: 0.2.0.9000
Authors@R: c(
  person("Matthew", "Lincoln", email = "matthew.d.lincoln@gmail.com", role = c("aut", "cre")),
